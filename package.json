    "scripts": {
        "start": "craco start",
        "build": "craco build",
        "test": "craco test",
        "eject": "react-scripts eject",
        "prepare": "husky install",
        "pre-commit": "lint-staged"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "rules": {
            "jsx-a11y/anchor-is-valid": "off"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "prettier": {
        "printWidth": 120,
        "trailingComma": "all",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "bracketSpacing": true,
        "arrowParens": "always",
        "jsxBracketSameLine": true
    },
    "devDependencies": {
        "husky": "^6.0.0",
        "lint-staged": "^10.5.4",
        "pretty-quick": "^3.1.0"
    },
    "lint-staged": {
        "**/*.js": [
            "./node_modules/.bin/eslint src --max-warnings=0",
            "pretty-quick --staged"
        ]
    }
}
